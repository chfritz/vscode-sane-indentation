{
  "name": "sane-indentation",
  "displayName": "sane-indentation",
  "description": "Uses the parse-tree to sanely indent lines",
  "version": "0.1.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/chfritz/vscode-sane-indentation"
  },
  "publisher": "cfritz",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sane-indentation.indentLine"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sane-indentation.indentLine",
        "title": "Sanely indent line"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.68.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  },
  "extensionDependencies": [
    "pokey.parse-tree"
  ]
}
